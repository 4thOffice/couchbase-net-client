{
    "_id": "[[DESIGNDOC]]",
    "views": {
        "by_name": {
            "map": "function (doc) { if (doc.type == \"city\") { emit(doc.name, null); } }"
        },
        "by_city_and_state": {
            "map": "function (doc) { if (doc.type == \"city\") { emit(null, { \"cityState\" : doc.name + \",\" + doc.state }); } }"
        }
    }
}