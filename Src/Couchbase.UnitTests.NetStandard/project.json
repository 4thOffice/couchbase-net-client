  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dnxcore50",

      "dependencies": {
        "Couchbase.NetStandard": {
          "target": "project",
          "version": "1.0.0-*"
        },
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0-*"
        }
      }
    }
  },

  "testRunner": "nunit"
}